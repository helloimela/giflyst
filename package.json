{
  "name": "giflyst",
  "private": true,
  "version":"0.0.0",
  "devDependencies": {
    "lerna": "^3.22.1",
    "lerna-changelog": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloimela/giflyst.git"
  },
  "scripts": {
    "install": "lerna bootstrap",
    "start": "lerna run --parallel start",
    "build": "lerna run build",
    "changelog": "lerna-changelog",
    "clean": "lerna clean",
    "version": "lerna version",
    "prepublish": "lerna run build",
    "release": "lerna publish"
  },
  "changelog": {
    "ignoreCommitters": [
      "dependabot"
    ],
    "labels": {
      "Feature": ":rocket: New Feature",
      "Improvement": ":seedling: Improvements",
      "bug": ":bug: Bug Fix"
    }
  }
}
